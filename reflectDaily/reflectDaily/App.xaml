<?xml version="1.0" encoding="utf-8" ?>
<Application xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="reflectDaily.App">

    <Application.Resources>

        <ResourceDictionary>

            <Color x:Key="primary">#B2DFDC</Color>
            <Color x:Key="secondary">#7EB4B0</Color>
            <Color x:Key="accent">#FFDD67</Color>
            <Color x:Key="neutral">#060300</Color>
            <Color x:Key="base">#fff5ff</Color>
            <Color x:Key="info">#E0E0E0</Color>
            <Color x:Key="success">#62B8B3</Color>
            <Color x:Key="warning">#BEE9E6</Color>
            <Color x:Key="error">#C33737</Color>


        </ResourceDictionary>

    </Application.Resources>
</Application>